    "import cv2\n",
    "import mediapipe as mp\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "from moviepy.editor import VideoFileClip\n",
    "\n",
    "# Reset counters and temporary lists\n",
    "frame_count1 = 0\n",
    "angles_list_knee = []\n",
    "angles_list_hip = []\n",
    "\n",
    "text_color_standsit = (255, 255, 255)  # Set a default value\n",
    "\n",
    "# 2. Second iteration through the video to analyze angles\n",
    "cap = cv2.VideoCapture(\"Video/file/path\")\n",
    "\n",
    "counter = 0\n",
    "stage = None\n",
    "\n",
    "while cap.isOpened():\n",
    "    ret, frame = cap.read()\n",
    "    if not ret:\n",
    "        break\n",
    "\n",
    "    # Get the original frame's width and height\n",
    "    original_height, original_width = frame.shape[:2]\n",
    "\n",
    "    # Determine the adjusted size\n",
    "    if original_width >= original_height:\n",
    "        new_width = target_long_side\n",
    "        new_height = int(original_height * (target_long_side / original_width))\n",
    "    else:\n",
    "        new_height = target_long_side\n",
    "        new_width = int(original_width * (target_long_side / original_height))\n",
    "\n",
    "    # Resize the frame\n",
    "    frame = cv2.resize(frame, (new_width, new_height), interpolation=cv2.INTER_AREA)\n",
    "\n",
    "    # Calculate remaining padding positions\n",
    "    top_pad = (target_long_side - new_height) // 2\n",
    "    bottom_pad = target_long_side - new_height - top_pad\n",
    "    left_pad = (target_long_side - new_width) // 2\n",
    "    right_pad = target_long_side - new_width - left_pad\n",
    "\n",
    "    # Pad the frame around\n",
    "    frame = cv2.copyMakeBorder(frame, top_pad, bottom_pad, left_pad, right_pad, cv2.BORDER_CONSTANT, value=(0, 0, 0))\n",
    "\n",
    "    # Recolor image to RGB\n",
    "    image = cv2.cvtColor(frame, cv2.COLOR_BGR2RGB)\n",
    "    image.flags.writeable = False\n",
    "\n",
    "    # Make detection\n",
    "    results = pose.process(image)\n",
    "\n",
    "    # Recolor back to BGR\n",
    "    image.flags.writeable = True\n",
    "    image = cv2.cvtColor(image, cv2.COLOR_RGB2BGR)\n",
    "    \n",
    "    # Render detections\n",
    "    mp_drawing = mp.solutions.drawing_utils\n",
    "    mp_drawing.draw_landmarks(image, results.pose_landmarks, mp_pose.POSE_CONNECTIONS,\n",
    "                                mp_drawing.DrawingSpec(color=(245, 117, 66), thickness=2, circle_radius=2),\n",
    "                                mp_drawing.DrawingSpec(color=(245, 66, 230), thickness=2, circle_radius=2))\n",
    "    # Extract landmarks\n",
    "    try:\n",
    "        landmarks = results.pose_world_landmarks.landmark\n",
    "        \n",
    "        # Get 3D coordinates of right foot shoulder, hip, knee\n",
    "        right_shoulder = np.array([results.pose_world_landmarks.landmark[11].x,\n",
    "                                    results.pose_world_landmarks.landmark[11].y,\n",
    "                                    results.pose_world_landmarks.landmark[11].z])\n",
    "        \n",
    "        right_hip = np.array([results.pose_world_landmarks.landmark[23].x,\n",
    "                                results.pose_world_landmarks.landmark[23].y,\n",
    "                                results.pose_world_landmarks.landmark[23].z])\n",
    "\n",
    "        right_knee = np.array([results.pose_world_landmarks.landmark[25].x,\n",
    "                                results.pose_world_landmarks.landmark[25].y,\n",
    "                                results.pose_world_landmarks.landmark[25].z])\n",
    "        \n",
    "        right_ankle = np.array([results.pose_world_landmarks.landmark[27].x,\n",
    "                                results.pose_world_landmarks.landmark[27].y,\n",
    "                                results.pose_world_landmarks.landmark[27].z])\n",
    "        \n",
    "        left_shoulder = np.array([results.pose_world_landmarks.landmark[12].x,\n",
    "                                    results.pose_world_landmarks.landmark[12].y,\n",
    "                                    results.pose_world_landmarks.landmark[12].z])\n",
    "\n",
    "        left_hip = np.array([results.pose_world_landmarks.landmark[24].x,\n",
    "                                results.pose_world_landmarks.landmark[24].y,\n",
    "                                results.pose_world_landmarks.landmark[24].z])\n",
    "\n",
    "        left_knee = np.array([results.pose_world_landmarks.landmark[26].x,\n",
    "                                results.pose_world_landmarks.landmark[26].y,\n",
    "                                results.pose_world_landmarks.landmark[26].z])\n",
    "        \n",
    "        left_ankle = np.array([results.pose_world_landmarks.landmark[28].x,\n",
    "                                results.pose_world_landmarks.landmark[28].y,\n",
    "                                results.pose_world_landmarks.landmark[28].z])\n",
    "\n",
    "        # Calculate angles in radians\n",
    "        vector1 = right_hip - right_knee\n",
    "        vector2 = right_ankle - right_knee\n",
    "        dot_product_knee = np.dot(vector1, vector2)\n",
    "        norm_product_knee = np.linalg.norm(vector1) * np.linalg.norm(vector2)\n",
    "        cos_theta_knee = dot_product_knee / norm_product_knee\n",
    "        angle_rad_knee = np.arccos(np.clip(cos_theta_knee, -1.0, 1.0))\n",
    "\n",
    "        # Calculate angles in radians\n",
    "        vector3 = left_hip - left_knee\n",
    "        vector4 = left_ankle - left_knee\n",
    "        dot_product_lknee = np.dot(vector3, vector4)\n",
    "        norm_product_lknee = np.linalg.norm(vector3) * np.linalg.norm(vector4)\n",
    "        cos_theta_lknee = dot_product_lknee / norm_product_lknee\n",
    "        angle_rad_lknee = np.arccos(np.clip(cos_theta_lknee, -1.0, 1.0))\n",
    "\n",
    "        # Calculate angles in radians\n",
    "        vector5 = right_shoulder - right_hip\n",
    "        vector6 = right_knee - right_hip\n",
    "        dot_product_hip = np.dot(vector5, vector6)\n",
    "        norm_product_hip = np.linalg.norm(vector5) * np.linalg.norm(vector6)\n",
    "        cos_theta_hip = dot_product_hip / norm_product_hip\n",
    "        angle_rad_hip = np.arccos(np.clip(cos_theta_hip, -1.0, 1.0))\n",
    "\n",
    "        # Calculate angles in radians\n",
    "        vector7 = left_shoulder - left_hip\n",
    "        vector8 = left_knee - left_hip\n",
    "        dot_product_lhip = np.dot(vector7, vector8)\n",
    "        norm_product_lhip = np.linalg.norm(vector7) * np.linalg.norm(vector8)\n",
    "        cos_theta_lhip = dot_product_lhip / norm_product_lhip \n",
    "        angle_rad_lhip = np.arccos(np.clip(cos_theta_lhip, -1.0, 1.0))\n",
    "\n",
    "        # Convert radians to degrees\n",
    "        angle_deg_knee = np.degrees(angle_rad_knee)\n",
    "        angle_deg_lknee = np.degrees(angle_rad_lknee)\n",
    "        angle_deg_hip = np.degrees(angle_rad_hip)\n",
    "        angle_deg_lhip = np.degrees(angle_rad_lhip)\n",
    "\n",
    "        angle_hip = (angle_deg_hip + angle_deg_lhip) / 2\n",
    "        angle_knee = (angle_deg_knee + angle_deg_lknee) / 2\n",
    "\n",
    "        # Add angles to the list\n",
    "        angles_list_knee.append(angle_knee)\n",
    "        angles_list_hip.append(angle_hip)\n",
    "\n",
    "        # Check if the angle exceeds the threshold\n",
    "        if angle_knee > threshold_high_knee:\n",
    "            text_color_knee = (0, 255, 0)  # Green\n",
    "        elif angle_knee < threshold_low_knee:\n",
    "            text_color_knee = (255, 255, 255)  # White\n",
    "        else:\n",
    "            text_color_knee = (255, 0, 0)  # Blue\n",
    "\n",
    "        if angle_hip > threshold_high_hip:\n",
    "            text_color_hip = (0, 255, 0)  # Green\n",
    "        elif angle_hip < threshold_low_hip:\n",
    "            text_color_hip = (255, 255, 255)  # White\n",
    "        else:\n",
    "            text_color_hip = (255, 0, 0)  # Blue\n",
    "\n",
    "        if angle_deg_lhip - angle_deg_hip > 20:\n",
    "            text_color_pose = (0, 0, 255)  # Red\n",
    "        elif angle_deg_lhip - angle_deg_hip > 20:\n",
    "            text_color_pose = (0, 0, 255)  # Red\n",
    "        else:\n",
    "            text_color_pose = None\n",
    "\n",
    "        # Setup status box\n",
    "        cv2.rectangle(image, (0, 0), (160, 50), (245, 117, 16), -1)\n",
    "\n",
    "        # Setup status box BLACK\n",
    "        cv2.rectangle(image, (5, 60), (195, 205), (0, 0, 0), -1)\n",
    "\n",
    "        text_position_knee = (10, 80)\n",
    "        cv2.putText(image, f\"Knee Angle : {angle_knee:.1f}\", text_position_knee, cv2.FONT_HERSHEY_SIMPLEX, 0.6, text_color_knee, 2, cv2.LINE_AA)\n",
    "\n",
    "        text_position_lknee = (10, 105)\n",
    "        cv2.putText(image, f\"Hip Angle : {angle_hip:.1f}\", text_position_lknee, cv2.FONT_HERSHEY_SIMPLEX, 0.6, text_color_hip, 2, cv2.LINE_AA)\n",
    "\n",
    "        # Curl counter logic\n",
    "        if angle_hip > threshold_high_hip:\n",
    "            if stage == \"sit\":\n",
    "                stage = \"stand\"\n",
    "                counter += 1\n",
    "                print(counter)\n",
    "            text_color_standsit = (0, 255, 0)\n",
    "        elif angle_hip < threshold_low_hip:\n",
    "            if stage != \"sit\":  # If the current stage is not \"sit,\" it means entering the \"sit\" stage\n",
    "                stage = \"sit\"\n",
    "                text_color_standsit = (255, 255, 255)\n",
    "\n",
    "        # Rep data\n",
    "        cv2.putText(image, 'REPS', (10, 12), cv2.FONT_HERSHEY_SIMPLEX, 0.5, (0, 0, 0), 1, cv2.LINE_AA)\n",
    "        cv2.putText(image, str(counter), (14, 45), cv2.FONT_HERSHEY_SIMPLEX, 1, (255, 255, 255), 2, cv2.LINE_AA)\n",
    "\n",
    "        # Stage data\n",
    "        cv2.putText(image, 'STAGE', (60, 12), cv2.FONT_HERSHEY_SIMPLEX, 0.5, (0, 0, 0), 1, cv2.LINE_AA)\n",
    "        cv2.putText(image, stage, (62, 43), cv2.FONT_HERSHEY_SIMPLEX, 1, text_color_standsit, 2, cv2.LINE_AA)\n",
    "\n",
    "    except Exception as e:\n",
    "        print(f\"Error: {e}\")\n",
    "\n",
    "    # Display the processed image\n",
    "    cv2.imshow('Mediapipe Feed', image)\n",
    "\n",
    "    # Press 'q' to exit\n",
    "    if cv2.waitKey(1) & 0xFF == ord('q'):\n",
    "        break\n",
    "\n",
    "# Release the video source\n",
    "cap.release()\n",
    "cv2.destroyAllWindows()\n",
    "\n",
    "# Plot angle variation chart\n",
    "plt.plot(angles_list_knee, label='Knee Angle')\n",
    "plt.axhline(y=threshold_high_knee, color='r', linestyle='--', label='Threshold High')\n",
    "plt.axhline(y=threshold_low_knee, color='b', linestyle='--', label='Threshold Low')\n",
    "plt.title('Knee Angle Changes Over Frames')\n",
    "plt.xlabel('Frame Index')\n",
    "plt.ylabel('Right Knee Angle (degrees)')\n",
    "# Add threshold labels to the chart\n",
    "plt.text(0, threshold_high_knee, f' {round(threshold_high_knee, 1)}', color='r', fontsize=10, verticalalignment='bottom')\n",
    "plt.text(0, threshold_low_knee, f'{round(threshold_low_knee, 1)}', color='b', fontsize=10, verticalalignment='top')\n",
    "# Show legend\n",
    "plt.legend()\n",
    "# Show the chart\n",
    "plt.show()\n",
    "\n",
    "# Plot angle variation for each frame\n",
    "frames = range(1, len(angles_list_hip) + 1)\n",
    "plt.plot(frames, angles_list_hip, marker='o', label='Hip Angle')\n",
    "# Add threshold lines at y=10 and y=-10 with red dashed lines\n",
    "plt.axhline(y=threshold_high_hip, color='r', linestyle='--', label='Threshold High')\n",
    "plt.axhline(y=threshold_low_hip, color='g', linestyle='--', label='Threshold Low')\n",
    "plt.xlabel('Hip Angle Changes Over Frames')\n",
    "plt.ylabel('Hip Angle (degrees)')\n",
    "plt.title('Hip Angle Variation Over Frames')\n",
    "# Add threshold labels to the chart\n",
    "plt.text(1, threshold_high_hip, f'{round(threshold_high_hip, 1)}', color='r', fontsize=10, verticalalignment='bottom')\n",
    "plt.text(1, threshold_low_hip, f'{round(threshold_low_hip, 1)}', color='g', fontsize=10, verticalalignment='top')\n",
    "# Show legend\n",
    "plt.legend()\n",
    "# Show the chart\n",
    "plt.show()\n",
    "\n",
    "# Output final counters\n",
    "print(f\"Final sit to stand: {counter}\")\n",
    "\n",
    "# Get video duration\n",
    "from moviepy.editor import VideoFileClip\n",
    "\n",
    "def get_video_duration(file_path):\n",
    "    try:\n",
    "        clip = VideoFileClip(file_path)\n",
    "        duration = clip.duration\n",
    "        clip.close()\n",
    "        return duration\n",
    "    except Exception as e:\n",
    "        return f\"Error: {e}\"\n",
    "\n",
    "# Call the function with the file path\n",
    "video_file_path = \"Video/file/path\"\n",
    "duration = get_video_duration(video_file_path)\n",
    "\n",
    "if isinstance(duration, float):\n",
    "    print(f\"The duration of the video is {duration} seconds.\")\n",
    "else:\n",
    "    print(duration)\n",
    "\n",
    "# Calculate average time per cycle\n",
    "if counter > 0:\n",
    "    average_time = duration / counter\n",
    "    print(f\"Average time per sit-to-stand cycle: {average_time} seconds.\")\n",
    "else:\n",
    "    print(\"No sit-to-stand cycles detected.\")"
