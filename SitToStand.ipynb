    "import cv2\n",
    "import mediapipe as mp\n",
    "import numpy as np\n",
    "from moviepy.editor import VideoFileClip\n",
    "\n",
    "mp_pose = mp.solutions.pose\n",
    "\n",
    "# Setup mediapipe instance\n",
    "pose = mp_pose.Pose(static_image_mode=False,\n",
    "                    model_complexity=2,\n",
    "                    smooth_landmarks=True,\n",
    "                    min_detection_confidence=0.5,\n",
    "                    min_tracking_confidence=0.5)\n",
    "\n",
    "# Use a video file as the source\n",
    "cap = cv2.VideoCapture(\"Video/file/path\")\n",
    "\n",
    "# Set a new width\n",
    "target_long_side = 1024\n",
    "\n",
    "# 1. First iteration through the video to find min and max angles\n",
    "min_angle_knee = float('inf')\n",
    "max_angle_knee = float('-inf')\n",
    "min_angle_hip = float('inf')\n",
    "max_angle_hip = float('-inf')\n",
    "\n",
    "# Reset counters and temporary lists\n",
    "frame_count = 0\n",
    "temp_min_values_knee = []\n",
    "temp_max_values_knee = []\n",
    "temp_min_values_hip = []\n",
    "temp_max_values_hip = []\n",
    "angles_list_knee = []\n",
    "angles_list_hip = []\n",
    "\n",
    "# Process frames in a loop\n",
    "while cap.isOpened():\n",
    "    ret, frame = cap.read()\n",
    "\n",
    "    if not ret:\n",
    "        break\n",
    "\n",
    "    # Pose estimation\n",
    "    results = pose.process(frame)\n",
    "\n",
    "    # Check if pose detection is successful\n",
    "    if results.pose_landmarks is not None:\n",
    "        landmarks = results.pose_world_landmarks.landmark\n",
    "\n",
    "        # Get 3D coordinates of right foot shoulder, hip, and knee\n",
    "        right_shoulder = np.array([landmarks[11].x, landmarks[11].y, landmarks[11].z])\n",
    "        right_hip = np.array([landmarks[23].x, landmarks[23].y, landmarks[23].z])\n",
    "        right_knee = np.array([landmarks[25].x, landmarks[25].y, landmarks[25].z])\n",
    "        right_ankle = np.array([landmarks[27].x, landmarks[27].y, landmarks[27].z])\n",
    "\n",
    "        # Get 3D coordinates of left foot shoulder, hip, and knee\n",
    "        left_shoulder = np.array([landmarks[12].x, landmarks[12].y, landmarks[12].z])\n",
    "        left_hip = np.array([landmarks[24].x, landmarks[24].y, landmarks[24].z])\n",
    "        left_knee = np.array([landmarks[26].x, landmarks[26].y, landmarks[26].z])\n",
    "        left_ankle = np.array([landmarks[28].x, landmarks[28].y, landmarks[28].z])\n",
    "\n",
    "        # Calculate angles in radians\n",
    "        vector1 = right_hip - right_knee\n",
    "        vector2 = right_ankle - right_knee\n",
    "        dot_product_knee = np.dot(vector1, vector2)\n",
    "        norm_product_knee = np.linalg.norm(vector1) * np.linalg.norm(vector2)\n",
    "        cos_theta_knee = dot_product_knee / norm_product_knee\n",
    "        angle_rad_knee = np.arccos(np.clip(cos_theta_knee, -1.0, 1.0))\n",
    "\n",
    "        vector3 = left_hip - left_knee\n",
    "        vector4 = left_ankle - left_knee\n",
    "        dot_product_lknee = np.dot(vector3, vector4)\n",
    "        norm_product_lknee = np.linalg.norm(vector3) * np.linalg.norm(vector4)\n",
    "        cos_theta_lknee = dot_product_lknee / norm_product_lknee\n",
    "        angle_rad_lknee = np.arccos(np.clip(cos_theta_lknee, -1.0, 1.0))\n",
    "\n",
    "        vector5 = right_shoulder - right_hip\n",
    "        vector6 = right_knee - right_hip\n",
    "        dot_product_hip = np.dot(vector5, vector6)\n",
    "        norm_product_hip = np.linalg.norm(vector5) * np.linalg.norm(vector6)\n",
    "        cos_theta_hip = dot_product_hip / norm_product_hip\n",
    "        angle_rad_hip = np.arccos(np.clip(cos_theta_hip, -1.0, 1.0))\n",
    "\n",
    "        vector7 = left_shoulder - left_hip\n",
    "        vector8 = left_knee - left_hip\n",
    "        dot_product_lhip = np.dot(vector7, vector8)\n",
    "        norm_product_lhip = np.linalg.norm(vector7) * np.linalg.norm(vector8)\n",
    "        cos_theta_lhip = dot_product_lhip / norm_product_lhip\n",
    "        angle_rad_lhip = np.arccos(np.clip(cos_theta_lhip, -1.0, 1.0))\n",
    "\n",
    "        # Convert radians to degrees\n",
    "        angle_deg_knee = np.degrees(angle_rad_knee)\n",
    "        angle_deg_lknee = np.degrees(angle_rad_lknee)\n",
    "        angle_deg_hip = np.degrees(angle_rad_hip)\n",
    "        angle_deg_lhip = np.degrees(angle_rad_lhip)\n",
    "\n",
    "        # Add angles to the list\n",
    "        angles_list_knee.append(angle_deg_knee)\n",
    "        angles_list_knee.append(angle_deg_lknee)\n",
    "        angles_list_hip.append(angle_deg_hip)\n",
    "        angles_list_hip.append(angle_deg_lhip)\n",
    "\n",
    "        # Update min and max values\n",
    "        min_angle_knee = min(min_angle_knee, angle_deg_knee, angle_deg_lknee)\n",
    "        max_angle_knee = max(max_angle_knee, angle_deg_knee, angle_deg_lknee)\n",
    "        min_angle_hip = min(min_angle_hip, angle_deg_hip, angle_deg_lhip)\n",
    "        max_angle_hip = max(max_angle_hip, angle_deg_hip, angle_deg_lhip)\n",
    "\n",
    "        # Reset counters and temporary lists\n",
    "        frame_count += 1\n",
    "        temp_min_values_knee.append(min_angle_knee)\n",
    "        temp_max_values_knee.append(max_angle_knee)\n",
    "        temp_min_values_hip.append(min_angle_hip)\n",
    "        temp_max_values_hip.append(max_angle_hip)\n",
    "\n",
    "        # Process every 200 frames\n",
    "        if frame_count % 200 == 0:\n",
    "            # Calculate averages\n",
    "            avg_min_knee = np.mean(temp_min_values_knee)\n",
    "            avg_max_knee = np.mean(temp_max_values_knee)\n",
    "            avg_min_hip = np.mean(temp_min_values_hip)\n",
    "            avg_max_hip = np.mean(temp_max_values_hip)\n",
    "\n",
    "            # Ensure threshold_high_knee is not less than 150\n",
    "            avg_max_knee = max(avg_max_knee, 150)\n",
    "\n",
    "            # Ensure threshold_low_knee is not greater than 150\n",
    "            avg_min_knee = min(avg_min_knee, 150)\n",
    "\n",
    "            # Check if min value is within the average range\n",
    "            if min_angle_knee < avg_min_knee:\n",
    "                angles_list_knee.append(min_angle_knee)\n",
    "\n",
    "            # Check if max value is within the average range\n",
    "            if max_angle_knee > avg_max_knee:\n",
    "                angles_list_knee.append(max_angle_knee)\n",
    "\n",
    "            # Ensure threshold_high_hip is not less than 150\n",
    "            avg_max_hip = max(avg_max_hip, 150)\n",
    "\n",
    "            # Ensure threshold_low_hip is not greater than 150\n",
    "            avg_min_hip = min(avg_min_hip, 150)\n",
    "\n",
    "            # Check if min value is within the average range\n",
    "            if min_angle_hip < avg_min_hip:\n",
    "                angles_list_hip.append(min_angle_hip)\n",
    "\n",
    "            # Check if max value is within the average range\n",
    "            if max_angle_hip > avg_max_hip:\n",
    "                angles_list_hip.append(max_angle_hip)\n",
    "\n",
    "            # Reset counters and temporary lists\n",
    "            frame_count = 0\n",
    "            temp_min_values_knee = []\n",
    "            temp_max_values_knee = []\n",
    "            temp_min_values_hip = []\n",
    "            temp_max_values_hip = []\n",
    "\n",
    "# Release the video source\n",
    "cap.release()\n",
    "\n",
    "# Set thresholds\n",
    "threshold_high_knee = max_angle_knee - 10\n",
    "threshold_low_knee = min_angle_knee + 30\n",
    "threshold_high_hip = max_angle_hip - 10\n",
    "threshold_low_hip = min_angle_hip + 30\n",
    "\n",
    "print(f\"Threshold High Knee: {round(threshold_high_knee, 1)}\")\n",
    "print(f\"Threshold Low Knee: {round(threshold_low_knee, 1)}\")\n",
    "print(f\"Threshold High Hip: {round(threshold_high_hip, 1)}\")\n",
    "print(f\"Threshold Low Hip: {round(threshold_low_hip, 1)}\")"
